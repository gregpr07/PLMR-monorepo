{
  "seed": 0,
  "gamma": 0.99,
  "alpha": 1,
  "soft_update": false,
  "offline": false,
  "target_update_frequency": 1,
  "actor_update_frequency": 1,
  "initial_mem": 10,
  "env": {
    "name": "HalfCheetah-v4"
  },
  "expert": {
    "num_trajs": 4,
    "max_traj_steps": 1000,
    "model_dir": "./models/cheetah.zip"
  },
  "train": {
    "epochs": 100,
    "episode_steps": 100,
    "warmup_steps": 10,
    "cuda": false,
    "mps": false,
    "lr": 0.01,
    "batch_size": 32,
    "log_interval": 10,
    "show_vis": false
  },
  "eval": {
    "num_trajs": 10,
    "max_traj_steps": 1000,
    "show_vis": false
  },
  "critic": {
    "hidden_dim": 256,
    "hidden_depth": 2
  },
  "actor": {
    "hidden_dim": 256,
    "hidden_depth": 2,
    "log_std_bounds": [
      -5,
      2
    ]
  }
}
